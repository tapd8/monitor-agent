{
  "name": "os-monitor",
  "version": "1.0.0",
  "description": "monitor cpu,mem,disk...",
  "keywords": [
    "os"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean:dist": "lb-clean dist",
    "clean": "lb-clean dist && lb-clean logs && lb-clean *.pid && lb-clean cache",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node $NODE_DEBUG_OPTION .",
    "prepublishOnly": "npm run test",
    "run": "node $NODE_DEBUG_OPTION app.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.0.6",
    "@loopback/boot": "^1.0.3",
    "@loopback/context": "^1.0.1",
    "@loopback/core": "^1.0.1",
    "@loopback/openapi-v3": "^1.1.0",
    "@loopback/repository": "^1.0.3",
    "@loopback/rest": "^1.2.0",
    "@loopback/service-proxy": "^1.0.1",
    "hashcode": "^1.0.3",
    "jsonwebtoken": "^8.4.0",
    "log4js": "^3.0.6",
    "node-os-utils": "1.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^1.0.1",
    "@types/node": "^10.11.2",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1"
  }
}
